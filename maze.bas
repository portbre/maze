5 REM ** DECLARE / INIT VARIABLES
10 X= RND(-TI)
15 DIM GPD$(3) :DIM GX(3) : DIM GY(3) : DIM GC(3) : DIM GL(3)
20 GPD$(0)="l" : GPD$(1)="l" : GPD$(2)="l" : GPD$(3)="l" : REM GHOST PREV DIR
30 GX(0)=1    : GX(1)=1    : GX(2)=25   : GX(3)=25   : REM GHOST X POSN
40 GY(0)=1    : GY(1)=23   : GY(2)=1    : GY(3)=23   : REM GHOST Y POSN
45 GC(0)=7    : GC(1)=3    : GC(2)=3    : GC(3)=3    : REM GHOST COLOR
50 GL(0)=46   : GL(1)=46   : GL(2)=46   : GL(3)=46   : REM CHAR IN GHOSTS LAST POSN
60 PX=13 : PY=11 : REM PLAYER START POSITION
65 PPD$="l"      : REM PLAYER PREV DIR
75 SCR=0         : REM SCORE
80 WALL=102      : rem decimal screen code for wall (checker board)
85 MC=0          : REM MOVE COUNTER
90 PRINT chr$(147) : print "number of ghosts" : INPUT NG
95 REM ** DRAW MAZE
100 poke 646,5        : rem change foreground color to green
110 PRINT chr$(147)
120 gosub 5000
125 POKE 1024+(PY*40)+PX, 88 : POKE 55296+(PY*40)+PX, 1
130 REM ** READ CHAR FROM KEYBOARD
135 get KB$ : IF KB$="q" THEN 999
140 REM ** MOVE PLAYER
145 GOSUB 1000
148 REM ** PRINT SCORE
150 POKE 781,2:POKE 782,30:POKE 783,0:SYS 65520:PRINT SCR
152 REM ** MOVE GHOSTS
155 FOR J = 0 TO NG-1
160 GOSUB 2000
170 NEXT J
175 MC=MC+1 : IF MC/100=INT(MC/100) THEN POKE 1717,83:POKE 55989,10 : REM HEART
180 GOTO 130
900 REM ** END GAME
990 POKE 1024+(PY*40)+PX,86 : POKE 55296+(PY*40)+PX,10 : REM RED X
991 POKE 781,5:POKE 782,30:POKE 783,0:SYS 65520:PRINT "again?"
992 GET KB$
994 IF KB$="" THEN 992
996 IF KB$="y" THEN 20
999 PRINT CHR$(147) : PRINT "good bye!" : END
1000 REM ** MOVE PLAYER SUBROUTINE
1005 POKE 1024+(PY*40)+PX,32
1020 IF (KB$="a" OR (KB$="" AND PPD$="u")) AND PEEK(1024+((PY-1)*40)+PX) <> WALL THEN PY=PY-1 : PPD$="u"
1030 IF (KB$="z" OR (KB$="" AND PPD$="d")) AND PEEK(1024+((PY+1)*40)+PX) <> WALL THEN PY=PY+1 : PPD$="d"
1040 IF (KB$="," OR (KB$="" AND PPD$="l")) AND PEEK(1024+(PY*40)+PX-1) <> WALL THEN PX=PX-1 : PPD$="l"
1050 IF (KB$="." OR (KB$="" AND PPD$="r")) AND PEEK(1024+(PY*40)+PX+1) <> WALL THEN PX=PX+1 : PPD$="r"
1055 IF PEEK(1024+(PY*40)+PX) = 46 THEN SCR=SCR+10
1058 IF PEEK(1024+(PY*40)+PX) = 83 THEN SCR=SCR+100
1060 POKE 1024+(PY*40)+PX,88 : POKE 55296+(PY*40)+PX, 1
1999 RETURN
2000 REM ** MOVE GHOST SUBROUTINE
2010 G$="" : ND$=""
2020 POKE 1024+(GY(J)*40)+GX(J), GL(J)   : REM CHAR FROM GHOSTS LAST POSN
2025 POKE 55296+(GY(J)*40)+GX(J), 5      : REM COLOR=GREEN
2028 REM BUILD AVAILABLE GHOST DIRECTIONS STRING
2030 WU=PEEK(1024+((GY(J)-1)*40)+GX(J))  : REM 1 POSITION ABOVE GHOST
2040 IF WU <> WALL AND GPD$(J) <> "d" THEN G$="u" 
2050 WD=PEEK(1024+((GY(J)+1)*40)+GX(J))  : REM POSITION BELOW GHOST
2060 IF WD <> WALL AND GPD$(J) <> "u" THEN G$=G$+"d" 
2070 WR=PEEK(1024+(GY(J)*40)+GX(J)+1)    : REM POSITION TO RIGHT OF GHOST
2080 IF WR <> WALL AND GPD$(J) <> "l" THEN G$=G$+"r" 
2090 WL=PEEK(1024+(GY(J)*40)+GX(J)-1)    : REM POSITION TO LEFT OF GHOST
2100 IF WL <> WALL AND GPD$(J) <> "r" THEN G$=G$+"l" 
2110 REM PICK ONE OF AVAILABLE DIRECTIONS
2140 IF J <> 0 THEN 2150
2141 REM GHOST 0 IS A CHASER
2142 IF PY < GY(J) AND (MID$(G$,1,1)="u") THEN ND$="u"
2144 IF PY > GY(J) AND (MID$(G$,1,1)="d" OR MID$(G$,2,1)="d") THEN ND$="d"
2146 IF ND$="" AND PX>GX(J) AND (MID$(G$,1,1)="r" OR MID$(G$,2,1)="r" OR MID$(G$,3,1)="r") THEN ND$="r"
2148 IF ND$="" AND PX<GX(J) AND (MID$(G$,1,1)="l" OR MID$(G$,2,1)="l" OR MID$(G$,3,1)="l" OR MID$(G$,4,1)="l") THEN ND$="l"
2150 IF ND$="" THEN ND$=MID$(G$,INT(RND(1)*LEN(G$))+1,1) : REM PICK ONE AT RANDOM
2160 REM UPDATE GHOST SCREEN POSN USING NEW DIRECTION
2170 IF ND$="u" THEN GY(J)=GY(J)-1
2180 IF ND$="d" THEN GY(J)=GY(J)+1
2190 IF ND$="l" THEN GX(J)=GX(J)-1
2200 IF ND$="r" THEN GX(J)=GX(J)+1
2205 GL(J)=PEEK(1024+(GY(J)*40)+GX(J))
2208 IF GL(J)=65 THEN GX(J)=13 : GY(J)=11 : GL(J)=PEEK(1477) : REM GHOST/GHOST COLLISION
2210 POKE 1024+(GY(J)*40)+GX(J),65       : REM CHAR = SPADE
2220 POKE 55296+(GY(J)*40)+GX(J),GC(J)   : REM COLOR= GHOST COLOR
2225 IF PX=GX(J) AND PY=GY(J) THEN 990   : REM GHOST/PLAYER COLLISION
2235 GPD$(J)=ND$
2240 RETURN
